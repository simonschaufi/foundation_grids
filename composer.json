{
	"name": "simonschaufi/foundation-grids",
	"type": "typo3-cms-extension",
	"description": "Gridelements for ZURB foundation CSS Framework",
	"license": [
		"GPL-2.0+"
	],
	"keywords": ["TYPO3 CMS", "Gridelements"],
	"require": {
		"php": ">=7.0.0",
		"typo3/cms-core": "^7.6.0 || ^8.7.0"
	},
	"autoload": {
		"psr-4": {
			"Simonschaufi\\FoundationGrids\\": "Classes/"
		}
	},
	"replace": {
		"foundation_grids": "self.version",
		"typo3-ter/foundation-grids": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "foundation_grids"
		}
	}
}